  function prependToMenuItem(itemClass, icon) {

    const $menuItem = $(`.gc-account-user-menu ${itemClass}`);

    if ($menuItem.length) {
      $menuItem.find('a').prepend(icon);
    }

  }


  setTimeout(function () {

    const default_profile = $('.menu-item-profile img[src*=default_profile]');

    if (default_profile.length) {
      default_profile.hide();
      prependToMenuItem('.menu-item-profile', iconsMenu.profile_icon);
    }


    prependToMenuItem('.menu-item-notifications_button_small', iconsMenu.notifications_button_icon);
    prependToMenuItem('.menu-item-cms', iconsMenu.cms_icon);
    prependToMenuItem('.menu-item-teach', iconsMenu.teach_icon);
    prependToMenuItem('.menu-item-user', iconsMenu.users_icon);
    prependToMenuItem('.menu-item-tasks', iconsMenu.tasks_icon);
    prependToMenuItem('.menu-item-notifications', iconsMenu.notifications_icon);
    prependToMenuItem('.menu-item-sales', iconsMenu.sales_icon);
    prependToMenuItem('.menu-item-chatium', iconsMenu.app_icon);

  }, 50);
});